<h2>{{view.title}}</h2>

<div class="form-horizontal">
  <div class="control-group">
    <label for="name" class="control-label">Set name</label>
    <div class="controls">
      {{view Ember.TextField valueBinding="name" id="name" action="create"}}
    </div>
  </div>
  <div class="form-actions">
    <input {{action create}} type="submit" class="btn btn-primary" value="Create Set" />
    <input {{action cancel}} type="submit" class="btn" value="Cancel" />
  </div>
</div>
